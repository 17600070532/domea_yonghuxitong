.bodys
  width 100%
  overflow hidden
  .content1
    width 1200px
    margin 0 auto
    .h1_a
      width 1200px
      height 56px
      border-bottom 1px solid #F1F1F2
      margin-top 52px
      span
        display block
        font-size  24px
        color  #666666
        line-height  24px
        font-family PingFangSC-Medium

  .wrap
    width 1200px
    overflow hidden
    margin 0 auto
    .content2
      width 100%
      overflow hidden
      .list
        width 1200px
        height 56px
        border-bottom 1px solid #F1F1F2
        .list_span_uid
          width 112px
          font-size  16px
          color  #333333
          line-height  16px
          float left
          display block
          font-family PingFangSC-Regular
          margin-top 20px
          text-align left
        .list_span_number
          display block
          font-size  16px
          color  #333333
          margin-top 20px
          margin-left 93px
          float: left
          font-family PingFangSC-Regular
        .list_span_change_password
          display block
          width 100px
          height 32px
          text-align center
          line-height 32px
          background #E6E6E6
          border-radius 4px
          color #666666
          font-size 14px
          float right
          margin-top 12px
          margin-right 16px
          font-family PingFangSC-Regular
          cursor pointer
        .cal
          color #999999
        .backgrounds
          background #00A0E9
          color #ffffff
